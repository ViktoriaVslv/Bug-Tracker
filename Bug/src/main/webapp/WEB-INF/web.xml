<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"   
xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">

  <servlet>
      <servlet-name>LoginServlet</servlet-name>
      <servlet-class>controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  
    <servlet>
      <servlet-name>ListBug</servlet-name>
      <servlet-class>ListBug</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ListBug</servlet-name>
    <url-pattern>/ListBug</url-pattern>
  </servlet-mapping>
  
  <servlet>
      <servlet-name>LogoutServlet</servlet-name>
      <servlet-class>controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  
  
    <servlet>
      <servlet-name>Info</servlet-name>
      <servlet-class>controller.Info</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Info</servlet-name>
    <url-pattern>/Info</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Table</servlet-name>
    <servlet-class>controller.Table</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Table</servlet-name>
    <url-pattern>/Table</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>MakeBug</servlet-name>
    <servlet-class>controller.MakeBug</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MakeBug</servlet-name>
    <url-pattern>/MakeBug</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SigninServlet</servlet-name>
    <servlet-class>controller.SigninServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SigninServlet</servlet-name>
    <url-pattern>/SigninServlet</url-pattern>
  </servlet-mapping>

</web-app>
